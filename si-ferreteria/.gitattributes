*.blade.php diff=html
*.css diff=css
*.html diff=html
*.md diff=markdown
*.php diff=php

/.github export-ignore
CHANGELOG.md export-ignore
.styleci.yml export-ignore

# Auto detect text files and normalize line endings to LF in the repo
* text=auto

# Force LF for these files in repo
*.php text eol=lf
*.blade.php text eol=lf
*.js text eol=lf
*.css text eol=lf
*.json text eol=lf
*.md text eol=lf

# StarUML files (binary-like, don't modify)
*.mdj binary

# Images
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.pdf binary
